@using Microsoft.AspNet.Identity
@{
    if (Request.IsAuthenticated && User.IsInRole("ADMIN"))
    {
        Layout = "~/Views/Shared/_Layout.cshtml";

    }
    else if (Request.IsAuthenticated)
    {

        Layout = "~/Views/Shared/_LayoutUser.cshtml";

    }
    else {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}
